# 环境配置示例文件
# 复制此文件为 .env.local 并根据实际情况修改

# 应用环境
NODE_ENV=development

# API 配置
REACT_APP_API_BASE_URL=http://localhost:8080/api
REACT_APP_MOCK_ENABLED=true

# 微应用配置
REACT_APP_MAIN_APP_PORT=3000
REACT_APP_REACT_MICRO_APP_PORT=3001
REACT_APP_VUE3_MICRO_APP_PORT=3002

# 微应用入口地址 (开发环境)
REACT_APP_REACT_MICRO_APP_ENTRY=//localhost:3001
REACT_APP_VUE3_MICRO_APP_ENTRY=//localhost:3002

# 微应用入口地址 (生产环境)
# REACT_APP_REACT_MICRO_APP_ENTRY=/react-micro-app
# REACT_APP_VUE3_MICRO_APP_ENTRY=/vue3-micro-app

# 监控配置
REACT_APP_SENTRY_DSN=
REACT_APP_ANALYTICS_ID=

# 功能开关
REACT_APP_ENABLE_MOCK=true
REACT_APP_ENABLE_DEVTOOLS=true
REACT_APP_ENABLE_PERFORMANCE_MONITOR=false

# CDN 配置
REACT_APP_CDN_BASE_URL=
REACT_APP_STATIC_BASE_URL=

# 认证配置
REACT_APP_AUTH_DOMAIN=
REACT_APP_AUTH_CLIENT_ID=

# 其他配置
REACT_APP_APP_VERSION=1.0.0
REACT_APP_BUILD_TIME=
