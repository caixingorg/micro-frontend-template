  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@enterprise/dev-tools": "workspace:*",
    "@enterprise/micro-app-sdk": "workspace:*",
    "@enterprise/monitoring-sdk": "workspace:*",
    "@enterprise/shared-components": "workspace:*",
    "@enterprise/shared-types": "workspace:*",
    "@enterprise/shared-utils": "workspace:*",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^5.12.8",
    "qiankun": "^2.10.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
